{
  "name": "d8x-app",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "eslint": "eslint src --ext \".js,.jsx,.ts,.tsx\"",
    "eslint-fix": "eslint src --ext \".js,.jsx,.ts,.tsx\" --fix",
    "stylelint": "npx stylelint \"src/**/*.scss\" --color",
    "stylelint-fix": "npx stylelint \"src/**/*.scss\" --color --fix",
    "lint:staged": "lint-staged",
    "lint:ts": "npm run eslint",
    "lint:format": "prettier --check './{src,cypress}/**/*.{ts,tsx}'",
    "lint:format:fix": "prettier --write './{src,cypress}/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "npm run stylelint",
    "lint": "npm run lint:ts && npm run lint:css",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npm run stylelint"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run prettify",
      "npm run eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@d8x/perpetuals-sdk": "0.8.2",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@ethersproject/abstract-provider": "5.7.0",
    "@ethersproject/abstract-signer": "5.7.0",
    "@ethersproject/bignumber": "5.7.0",
    "@ethersproject/bytes": "5.7.0",
    "@ethersproject/constants": "5.7.0",
    "@ethersproject/contracts": "5.7.0",
    "@ethersproject/strings": "5.7.0",
    "@ethersproject/units": "5.7.0",
    "@mui/icons-material": "5.13.7",
    "@mui/material": "5.11.16",
    "@rainbow-me/rainbowkit": "0.12.17",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "jotai": "2.0.4",
    "lightweight-charts": "4.0.1",
    "lightweight-charts-react-wrapper": "1.3.2",
    "node-sass": "8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-material-ui-carousel": "3.4.2",
    "react-resize-detector": "8.0.4",
    "react-router-dom": "6.9.0",
    "react-scripts": "5.0.1",
    "react-toastify": "9.1.2",
    "util": "0.12.5",
    "wagmi": "0.12.18"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "crypto-browserify": "3.12.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "https-browserify": "1.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "os-browserify": "0.3.0",
    "prettier": "2.8.7",
    "process": "0.11.10",
    "react-app-rewired": "2.2.1",
    "source-map-explorer": "2.5.3",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylelint": "14.16.1",
    "stylelint-config-css-modules": "4.2.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "6.0.3",
    "typescript": "4.9.5",
    "url": "0.11.0"
  },
  "proxy": "http://localhost:3001"
}
